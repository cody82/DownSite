<table>
  <% this.forEach(function(row){ %>
	<tr>
    <td>
      <input class="delete" id="<%= row.Id %>" type="checkbox"/>
    </td>
		<td>[](/index.html?article=/<%= row.Id %>)</td>
    <td>
      <a href="/index.html?article=<%= row %>"><%= row.Title %>
    </a>
    </td>
	</tr>
  <% }) %>
</table>